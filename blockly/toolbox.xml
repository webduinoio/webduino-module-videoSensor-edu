<category id="catVideoSensor">
  <block type="waspot_get_camera">
  </block>
  <block type='waspot_create_actor'>
    <value name="imgURL">
      <shadow type="text">
        <field name="TEXT">https://webduino.io/img/training/training4.png</field>
      </shadow>
    </value>
    <value name="width">
      <shadow type="math_number">
        <field name="NUM">64</field>
      </shadow>
    </value>
    <value name="height">
      <shadow type="math_number">
        <field name="NUM">64</field>
      </shadow>
    </value>
  </block>
  <block type='waspot_set_snd_url_actor'>
    <value name="sndURL">
      <shadow type="text">
        <field name="TEXT">http://webduino.tw/res/coin.mp3</field>
      </shadow>
    </value>
  </block>
  <block type='waspot_move_actor'>
    <value name="x">
      <shadow type="math_number">
        <field name="NUM">10</field>
      </shadow>
    </value>
    <value name="y">
      <shadow type="math_number">
        <field name="NUM">10</field>
      </shadow>
    </value>
  </block>
  <block type='waspot_touch_actor'>
  </block>
  <block type='waspot_show_actor'>
  </block>
  <block type='waspot_loadimg_actor'>
    <value name="imgURL">
      <shadow type="text">
        <field name="TEXT">https://webduino.io/img/training/training4.png</field>
      </shadow>
    </value>
  </block>
  <block type='waspot_reloadimg_actor'>
    <value name="imgURL">
      <shadow type="text">
        <field name="TEXT">https://webduino.io/img/training/training4.png</field>
      </shadow>
    </value>
    <value name="after">
      <shadow type="math_number">
        <field name="NUM">0.5</field>
      </shadow>
    </value>
  </block>
  <block type='waspot_set_image_size'>
    <value name="width">
      <shadow type="math_number">
        <field name="NUM">10</field>
      </shadow>
    </value>
    <value name="height">
      <shadow type="math_number">
        <field name="NUM">10</field>
      </shadow>
    </value>
  </block>
  <block type='waspot_play_audio_actor'>
  </block>
  <block type="waspot_ctrl_video">
    <value name="videoId">
      <block type="text">
        <field name="TEXT">http://webduino.tw/walkman.mp4</field>
      </block>
    </value>
  </block>
  <block type="waspot_setting_video">
  </block>
  <block type="waspot_ctrl_video_action">
  </block>
</category>